<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns:cm="http://aries.apache.org/blueprint/xmlns/blueprint-cm/v1.0.0"
           xsi:schemaLocation="
           http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">
           
    <!-- OSGI blueprint property placeholder -->
    <cm:property-placeholder id="persist.placeholder" persistent-id="camel.blueprint">
        <!-- list some properties for this test -->
        <cm:default-properties>
            <cm:property name="sms.endpoint.address" value="http://0.0.0.0"/>
            <cm:property name="sms.endpoint.port" value="8090"/>
            <cm:property name="sms.endpoint.name" value="smsinfo"/>
        </cm:default-properties>
    </cm:property-placeholder>

</blueprint>